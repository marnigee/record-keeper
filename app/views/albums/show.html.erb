<p>
  <strong>Title:</strong>
  <%= @album.title %>
</p>

<p>
  <strong>Year:</strong>
  <%= @album.year %>
</p>

<p>
  <strong>Artist:</strong>
  <%= @album.artist %>
</p>

<%= link_to 'Destroy', album_path(@album),
        method: :delete,
        data: { confirm: 'Are you sure?' } %>

<%= link_to '<- Back', albums_path %>

<%= link_to 'Edit', edit_album_path(@album) %>
